{
  "name": "base-bailey-json",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/app.js",
    "lint": "eslint . --no-ignore",
    "dev": "npm run lint && nodemon --signal SIGKILL  ./src/app.ts",
    "build": "npx rollup -c",
    "dev:watch": "nodemon --exec ts-node src/app.ts",
    "supervise": "node supervisor-simple.js",
    "supervise:ts": "ts-node supervisor.ts",
    "prod": "npm run build && npm run supervise",
    "supervisor": "ts-node supervisor.ts",
    "supervisor:build": "tsc && node dist/supervisor.js",
    "supervisor:prod": "npm run build && node supervisor.js",
    "win:supervise": "npm run build && npm run supervisor",
    "win:start": "node supervisor.js",
    "build:supervisor": "tsc supervisor.ts --outDir . --module commonjs --target es2020",
    "supervise:notify": "node supervisor-notifications.cjs",
    "prod:notify": "npm run build && npm run supervise:notify",
    "win:notify": "node supervisor-notifications.cjs",
    "notify:install": "npm install axios",
    "start:notify": "npm run notify:install && npm run supervise:notify"
  },
  "keywords": [],
  "dependencies": {
    "@builderbot/bot": "1.3.15",
    "@builderbot/provider-baileys": "1.3.15",
    "axios": "^1.13.2",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "mysql2": "^3.15.3",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.19.26",
    "@types/qrcode-terminal": "^0.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-builderbot": "latest",
    "nodemon": "^3.1.11",
    "rollup": "^4.10.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3"
  },
  "author": "",
  "license": "ISC"
}
